program 
	function public number factorial number n do
		number f;
		for f := 1 until n = 0 step n := n - 1 do
			f := f * n;
		done
		return f;
	function forward number atoi string s done; $EXTRN atoi$
	function forward number argc done; $EXTRN argc$
	function forward string argv number n done $EXTRN argv$
start
	if argc > 1 then:
		factorial(atoi(argv(1))) ! "\n" !
	fi
end
